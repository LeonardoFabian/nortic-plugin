{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nortic-plugin/alias",
  "title": "Alias",
  "category": "text",
  "description": "Add an alias textarea field for your post type",
  "keywords": [
    "name",
    "alias"
  ],
  "version": "1",
  "textdomain": "nortic-plugin",
  "editorScript": "file:./index.js",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "use_default_title": {
      "type": "boolean",
      "default": true
    }
  },
  "style": "file:./index.css",
  "usesContext": [
    "postId"
  ]
}