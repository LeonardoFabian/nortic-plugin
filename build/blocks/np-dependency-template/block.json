{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nortic-plugin/np-dependency-template",
  "title": "NP Dependency Template",
  "category": "widgets",
  "description": "Contains the block elements used to render a dependency, like the title, description, services, responsible member, contact and more.",
  "keywords": [
    "dependency",
    "dependencies",
    "template"
  ],
  "version": "1",
  "textdomain": "nortic-plugin",
  "editorScript": "file:./index.js",
  "attributes": {
    "numberOfPosts": {
      "type": "number",
      "default": 3
    },
    "categories": {
      "type": "array",
      "default": []
    },
    "taxonomies": {
      "type": "array",
      "default": []
    },
    "order": {
      "type": "string",
      "default": "desc"
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    },
    "displayFeaturedImage": {
      "type": "boolean",
      "default": true
    }
  },
  "style": "file:./index.css"
}