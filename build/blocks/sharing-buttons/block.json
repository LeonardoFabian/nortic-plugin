{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nortic-plugin/sharing-buttons",
  "title": "Sharing Buttons",
  "category": "text",
  "description": "Add share buttons to posts and pages",
  "keywords": [
    "share",
    "social",
    "buttons",
    "facebook",
    "twitter",
    "linkedin"
  ],
  "version": "1",
  "textdomain": "nortic-plugin",
  "editorScript": "file:./index.js",
  "attributes": {
    "justify_content": {
      "type": "string",
      "default": "flex-start"
    }
  },
  "style": "file:./index.css",
  "usesContext": [
    "postId"
  ]
}