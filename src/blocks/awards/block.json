{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "nortic-plugin/awards",
    "title": "Awards",
    "category": "text",
    "description": "Shows the recognitions obtained by your website.",
    "keywords": ["award", "recognition"],
    "version": "1",
    "textdomain": "nortic-plugin",
    "editorScript": "file:./index.js",
    "attributes": {
        "categories": {
            "type": "array", 
            "default": []
        },
        "count": {
            "type": "number",
            "default": 10
        },
        "orderBy": {
            "type": "string",
            "default": "title"
        },
        "order": {
            "type": "string",
            "default": "asc"
        }
    },
    "style": "file:./index.css"
}