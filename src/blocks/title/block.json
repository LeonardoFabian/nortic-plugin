{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "nortic-plugin/title",
    "title": "Title",
    "category": "text",
    "description": "Add a custom title",
    "keywords": ["title", "header", "underline"],
    "version": "1",
    "textdomain": "nortic-plugin",
    "editorScript": "file:./index.js",
    "attributes": {
        "title": {
            "type": "string",
            "source": "html",
            "selector": "h2",
            "default": ""
        },
        "color": {
            "type": "string",
            "default": "#003876"
        },
        "underline_color": {
            "type": "string",
            "default": "#ee2a24"
        }
    },
    "style": "file:./index.css"
}