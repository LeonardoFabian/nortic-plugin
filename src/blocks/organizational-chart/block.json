{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "nortic-plugin/organizational-chart",
    "title": "Organizational Chart",
    "category": "text",
    "description": "Add a customizable organizational chart to your site.",
    "keywords": ["chart", "organizational", "structure", "hierarchy"],
    "version": "1",
    "textdomain": "nortic-plugin",
    "editorScript": "file:./index.js",
    "attributes": {
        "endPoint": {
            "type": "string",
            "default": "http://test.mt.gob.do/wp-json/wp/v2"
        },
        "rootParentId": {
            "type": "number",
            "default": 0
        },
        "postType": {
            "type": "string",
            "default": "dependency"
        }
    },
    "style": "file:./index.css",
    "script": "file:./frontend-organizational-chart.js"
}